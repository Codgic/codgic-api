{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [true, 140],        
        "no-bitwise": false,
        "no-console": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}